<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.gdei.jobmessage.model">
	<class name="Login" table="login">
		<id name="userID" column="userID"></id>
		<property name="userPSW" column="userPSW"></property>
		<property name="question" column="question"></property>
		<property name="answer" column="answer"></property>
	</class>
</hibernate-mapping>