<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:background="#ffffff"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="36dp"
        android:text="招生就业处"
        android:textColor="#000000"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="14dp"
        android:text="版本 v_1.0"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp"
        android:text="制作：广东二师H13软工件工程班"
        android:textColor="#000000"
        android:textSize="20dp" />

     <RelativeLayout
         android:id="@+id/layout_top"
         android:layout_width="match_parent"
         android:layout_height="40dip"
         android:layout_below="@+id/tv4"
         android:layout_marginTop="16dp" >

         <LinearLayout
             android:id="@+id/linearLayout1"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content" >

             <TextView
                 android:id="@+id/textView1"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="  指导老师：朱雄泳  周如旗"
                 android:textColor="#000000" />

         </LinearLayout>

         <TextView
             android:id="@+id/textView2"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignParentBottom="true"
             android:layout_alignRight="@+id/linearLayout1"
             android:text="阮锦新  毕伟宏"
             android:textColor="#000000" />

     </RelativeLayout>

     <RelativeLayout
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_alignParentLeft="true"
         android:layout_alignRight="@+id/tv2"
         android:layout_below="@+id/layout_top"
         android:layout_marginTop="22dp" >

         <LinearLayout
             android:id="@+id/linearLayout2"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content" >

             <TextView
                 android:id="@+id/textView3"
                 android:layout_width="258dp"
                 android:layout_height="wrap_content"
                 android:text="  研发工程师：   陈建航  陈文辉 "
                 android:textColor="#000000" />
         </LinearLayout>

         <TextView
             android:id="@+id/textView7"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignLeft="@+id/textView6"
             android:layout_below="@+id/textView6"
             android:text="张敏怡  卢坤菲" 
             android:textColor="#000000" />

         <TextView
             android:id="@+id/textView8"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignLeft="@+id/textView7"
             android:layout_below="@+id/textView7"
             android:text="谢佳佳  肖鸣悦" 
              android:textColor="#000000"/>

         <TextView
             android:id="@+id/textView9"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignLeft="@+id/textView8"
             android:layout_centerVertical="true"
             android:text="刘珊珊"
             android:textColor="#000000" />

         <TextView
             android:id="@+id/textView4"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignParentRight="true"
             android:layout_below="@+id/linearLayout2"
             android:layout_marginRight="26dp"
             android:text="         林旭源  林威"
             android:textColor="#000000" />

         <TextView
             android:id="@+id/textView5"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignLeft="@+id/textView4"
             android:layout_below="@+id/textView4"
             android:text="         曹家荣  李始一"
             android:textColor="#000000" />

         <TextView
             android:id="@+id/textView6"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignRight="@+id/textView5"
             android:layout_below="@+id/textView5"
             android:text="潘凯宏  伍凯涛"
             android:textColor="#000000" />

     </RelativeLayout>

     <TextView
         android:id="@+id/tv4"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/tv1"
         android:layout_below="@+id/tv"
         android:text="软件开发兴趣小组"
         android:textColor="#000000"
         android:textSize="20dp" />

</RelativeLayout>